    "docker:build:local": "docker build . -t rdga-web:local",
    "docker:start:local": "docker run -d -p 81:80 rdga-web:local"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
